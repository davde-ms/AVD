{
  "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "customTags1": {
      "value": {}
    },
    "vmCount": {
      "value": 2
    },
    "vmPrefix": {
      "value": "Prefix used to build the VM name (cannot be longer than 11 characters)"
    },
    "vmStartNumber": {
      "value": 0
    },
    "vmSKU": {
      "value": "Standard_B2ms"
    },
    "localAdminUsername": {
      "reference": {
        "keyVault": {
          "id": "[extensionResourceId(format('/subscriptions/{0}/resourceGroups/{1}', format('{0}', parameters('avdSubscriptionId')), format('{1}', parameters('kvResourceGroupName'))), 'Microsoft.KeyVault/vaults', parameters('avdKvName'))]"
        },
        "secretName": "localAdminUsername"
      }
    },
    "localAdminPassword": {
      "reference": {
        "keyVault": {
          "id": "[extensionResourceId(format('/subscriptions/{0}/resourceGroups/{1}', format('{0}', parameters('avdSubscriptionId')), format('{1}', parameters('kvResourceGroupName'))), 'Microsoft.KeyVault/vaults', parameters('avdKvName'))]"
        },
        "secretName": "localAdminPassword"
      }
    },
    "useACG": {
      "value": false
    },
    "ACG_rg": {
      "value": ""
    },
    "ACG_Name": {
      "value": ""
    },
    "ACG_ImageDefinition_Name": {
      "value": ""
    },
    "diskType": {
      "value": "Premium_LRS"
    },
    "osDiskSize": {
      "value": 128
    },
    "availabilityOption": {
      "value": "AvailabilityZone"
    },
    "availabilitySetName": {
      "value": "Availability set your VMs will be in (if you choose AvailabilitySet)"
    },
    "aadJoin": {
      "value": true
    },
    "intune": {
      "value": false
    },
    "domain": {
      "value": "Active Directory domain name in GQDN format"
    },
    "OUPath": {
      "value": "LDAP path to the OU you want your VMs to be created in"
    },
    "domainJoinerUPN": {
      "reference": {
        "keyVault": {
          "id": "[extensionResourceId(format('/subscriptions/{0}/resourceGroups/{1}', format('{0}', parameters('avdSubscriptionId')), format('{1}', parameters('kvResourceGroupName'))), 'Microsoft.KeyVault/vaults', parameters('avdKvName'))]"
        },
        "secretName": "name of the secret containing the domain joiner UPN"
      }
    },
    "domainJoinerPassword": {
      "reference": {
        "keyVault": {
          "id": "[extensionResourceId(format('/subscriptions/{0}/resourceGroups/{1}', format('{0}', parameters('avdSubscriptionId')), format('{1}', parameters('kvResourceGroupName'))), 'Microsoft.KeyVault/vaults', parameters('avdKvName'))]"
        },
        "secretName": "name of the secret containing the domain joiner password"
      }
    },
    "artifacts_location": {
      "value": "https://wvdportalstorageblob.blob.core.windows.net/galleryartifacts/Configuration_01-19-2023.zip"
    },
    "hostpoolName": {
      "value": "name of the hostpool you want your VMs to join"
    },
    "hostpoolToken": {
      "reference": {
        "keyVault": {
          "id": "[extensionResourceId(format('/subscriptions/{0}/resourceGroups/{1}', format('{0}', parameters('avdSubscriptionId')), format('{1}', parameters('kvResourceGroupName'))), 'Microsoft.KeyVault/vaults', parameters('avdKvName'))]"
        },
        "secretName": "name of the secret containing the hostpool token"
      }
    },
    "subnetName": {
      "value": "name of the subnet you want to attach your VMs to"
    },
    "vNetId": {
      "value": "/subscriptions/xxxxxxxx-xxxx-xxxx-xxxx-xxxxxxxxxxxx/resourceGroups/your_vnet_resourcegroup/providers/Microsoft.Network/virtualNetworks/your_vnet_name"
    },
    "autoShutdownStatus": {
      "value": "Enabled"
    },
    "autoShutdownTime": {
      "value": "19:00"
    },
    "autoShutdownTimeZone": {
      "value": "Eastern Standard Time"
    }
  }
}
